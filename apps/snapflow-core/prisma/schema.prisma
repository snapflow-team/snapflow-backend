generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

//todo: разобраться почему подсвечивает ошибку если удаляешь свойство "url"!!!

datasource db {
  provider = "postgresql"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

  @@map("users")
}